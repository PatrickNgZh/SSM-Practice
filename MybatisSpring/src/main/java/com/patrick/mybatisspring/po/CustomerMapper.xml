<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.patrick.po.CustomerMapper">
    <select id="findCoutomerById" parameterType="Integer" resultType="customer">
        SELECT *
        FROM tb_customer
        WHERE id = #{id}
    </select>
</mapper>